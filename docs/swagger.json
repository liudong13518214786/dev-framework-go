{
    "swagger": "2.0",
    "info": {
        "description": "开发接口文档",
        "title": "gin脚手脚",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "kennyL",
            "email": "846723063@qq.com"
        },
        "license": {},
        "version": "1.0"
    },
    "host": "localhost:8890",
    "paths": {
        "/api/v1/blog": {
            "get": {
                "description": "获取博客列表",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "首页",
                "parameters": [
                    {
                        "type": "string",
                        "description": "页数",
                        "name": "p",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "关键词",
                        "name": "kw",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "搜索类型",
                        "name": "stype",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "code\": 100, \"msg\": \"success\", \"data\":[]}",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/cate": {
            "get": {
                "description": "博客的分类",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "分类",
                "responses": {
                    "200": {
                        "description": "code\": 100, \"msg\": \"success\", \"data\":[]}",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/detail": {
            "get": {
                "description": "博客详情",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "详情",
                "parameters": [
                    {
                        "type": "string",
                        "description": "博客id",
                        "name": "uuid",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "code\": 100, \"msg\": \"success\", \"data\":nil}",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/login": {
            "get": {
                "description": "通过用户的账号和密码登录",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "用户登录",
                "parameters": [
                    {
                        "type": "string",
                        "description": "用户名",
                        "name": "username",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "密码",
                        "name": "password",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "code\": 100, \"msg\": \"success\", \"data\":nil}",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/logout": {
            "get": {
                "description": "退出登录",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "退出登录",
                "responses": {
                    "200": {
                        "description": "code\": 100, \"msg\": \"success\", \"data\":nil}",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/upload": {
            "post": {
                "description": "上传博客头图",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "上传",
                "parameters": [
                    {
                        "type": "string",
                        "description": "用户名",
                        "name": "username",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "code\": 100, \"msg\": \"上传成功\", \"data\":\"图片地址\"}",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/user": {
            "get": {
                "description": "获取用户信息",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "获取用户信息",
                "responses": {
                    "200": {
                        "description": "code\": 100, \"msg\": \"success\", \"data\":{}}",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/write": {
            "post": {
                "description": "如果博客存在，那么则修改",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "编写博客",
                "parameters": [
                    {
                        "type": "string",
                        "description": "头图",
                        "name": "img",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "标题",
                        "name": "title",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "内容",
                        "name": "content",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "标签",
                        "name": "tags",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "博客id",
                        "name": "bid",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "code\": 100, \"msg\": \"success\", \"data\":nil}",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}